{
  "expo": {
    "name": "DoIKnowIt",
    "slug": "DoIKnowIt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#09A9A9"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon.png",
        "backgroundColor": "#09A9A9"
      },
      "splash": {
        "image": "./assets/images/app_icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#09A9A9"
      },
      "package": "com.patriciorosso.DoIKnowIt"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "55d07820-a061-435b-9d63-97dc54dac03c"
      }
    }
  }
}
